{
    "name": "eslintme-website",
    "version": "0.1.0",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "static": "next export",
        "start": "next start",
        "lint": "next lint",
        "licences": "license-report --output=json > public/licenses.json; npm run licences-core; npm run licences-native",
        "licences-core": "license-report --package lib/eslintme-core/package.json --output=json > public/licenses-core.json",
        "licences-native": "license-report --package ../eslintme-native-wrapper/package.json --output=json > public/licenses-native.json",
        "export-to-electron": "npm run export; rm -rf ../eslintme-native-wrapper/files; mv out/ ../eslintme-native-wrapper/files",
        "export": "node rules_downloader.js; npm run licences; npm run build; npm run static;"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@monaco-editor/react": "^4.3.1",
        "@mui/icons-material": "^5.3.0",
        "@mui/material": "^5.3.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-google": "^0.14.0",
        "eslint-config-standard": "^16.0.3",
        "espree": "^9.3.0",
        "md5": "^2.3.0",
        "next": "^12.0.8",
        "node-fetch": "^2.6.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-drag-drop-files": "^2.2.5",
        "react-dropzone": "^11.5.1",
        "sass": "^1.49.0"
    },
    "devDependencies": {
        "@types/md5": "^2.3.1",
        "@types/node": "^17.0.18",
        "@types/react": "17.0.30",
        "eslint-config-next": "11.1.2",
        "license-report": "^4.5.0",
        "typescript": "4.4.4"
    }
}
